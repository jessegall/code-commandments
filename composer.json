{
    "name": "jessegall/code-commandments",
    "description": "A Laravel package for enforcing code commandments through prophets who judge and absolve transgressions",
    "keywords": ["laravel", "code-quality", "static-analysis", "linting", "commandments"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jesse Gall",
            "email": "jessegall@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "nikic/php-parser": "^5.0",
        "symfony/finder": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "JesseGall\\CodeCommandments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JesseGall\\CodeCommandments\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JesseGall\\CodeCommandments\\CodeCommandmentsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
